{
	"name": "notadd/captcha",
	"type": "notadd-extension",
	"description": "Laravel 5 Captcha Package",
	"keywords": ["notadd Captcha", "notadd Security"],
	"homepage": "https://github.com/ganlanshu0211/captcha",
	"license": "MIT",
	"authors": [
		{
			"name": "cst",
			"email": "260096556@qq.com",
			"homepage": "https://github.com/ganlanshu0211",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"ext-gd": "*",
		"illuminate/config": "~5.0",
		"illuminate/filesystem": "~5.0",
		"illuminate/support": "~5.0",
		"illuminate/hashing": "~5.0",
		"intervention/image": "~2.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.1",
        "mockery/mockery": "0.9.*",
        "notadd/installers": "0.5.*"
	},
	"autoload": {
		"psr-4": {
			"Notadd\\Captcha\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		],
		"psr-4": {
			"Notadd\\Test\\": "tests/"
		}
	},
	"minimum-stability": "dev"
}
